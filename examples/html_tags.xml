<?xml version="1.0" encoding="UTF-8"?>
<uiml>
    <interface>
        <structure>
            <part class="div" id="div1">
                <part class="Anchor" id="link1"/>
                <part class="Anchor" id="link2">
                    <part class="Image" id="image1"/>  
                </part>
            </part>
            <part class="Image" id="image2">
                <style>
                    <property part-name="image2" name="src">image2.png</property>
                    <property part-name="image2" name="alt">nested picture as a button</property>
                </style>     
            </part>            
            <part class="Image" id="image3"/>  
        </structure>
        <style>
            <property part-name="link1" name="href">https://www.link1.com/</property>
            <property part-name="link1" name="text">link text</property>  
            <property part-name="link2" href="https://www.link2.com/" text="Click Here"/>
            <property part-name="image3" alt="this is alt" src="image3.png"/>           
            <property part-name="image1" name="src">image1.png</property>
            <property part-name="image1" name="alt">PIC BUTTON</property>
        </style>
    </interface>
    <peers>
    </peers>
</uiml>